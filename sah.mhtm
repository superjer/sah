<!DOCTYPE html>
<html>
<head>
<title>SuperJer Against Humanity</title>
<link rel=stylesheet type="text/css" href="css.css">
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<script src="ui/js/jquery-ui-1.10.3.custom.min.js"></script>
<script src="js.js"></script>
</head>
<body>

<div class=scoresheet>
  <h2>âŒ› <span class=clock>0</span></h2>
  <h1>SuperJer<br>Against<br>Humanity</h1>
  <table>
    <thead>
    <tr>
      <th>Player</th>
      <th>Pts</th>
      <th>Card</th>
    </tr>
    <thead>
    <tbody>
    </tbody>
  </table>
</div>

<div class=err></div>

<div class=leftcol>

<div class=holder>
  <div class="card blackcard" blackid=0>
    <div class=cardtxt></div>
    <div class="thermo love"><div style='height:20px;'></div></div>
    <div class=num>Play <div>0</div></div>
  </div>
</div>

<div class=play>
{{#slots}}
  <div class=holder slot={{.}}>
    <div class="card slot">
    </div>
  </div>
{{/slots}}
</div>

<div class="holder buttons">
  <div class=card>
    <button disabled class=callit>Call it</button>
    <button disabled class=draw>Draw</button>
  </div>
</div>

<div class=hand>
{{#hand}}
  <div class=holder slot={{.}}>
    <div class="card slot">
    </div>
  </div>
{{/hand}}
</div>

</div> <!-- leftcol -->

{{@reset}}
<button class=reset style='float:right;display:none;'>Reset</button>
{{/reset}}

<div class=shade></div>

<div class=selectwin>
  <div>
    <button class=abandon style='float:right;visibility:hidden;'>Abandon</button>
    <h1 class=wintitle>Choose winner...</h1>
    <div class=clear></div>
    <div class="card blackcard" blackid=0 style='width:200px;height:300px;'>
      <div class=cardtxt style='height:280px;'></div>
      <div class=num>Play <div>0</div></div>
    </div>
    <div class=contenders></div>
    <div class=clear></div>
  </div>
</div>

</body>
</html>
